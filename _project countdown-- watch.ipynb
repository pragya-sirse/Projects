   "source": [
    "import time\n",
    "t=int(input(\"enter number of seconds: \"))\n",
    "while t>=0:\n",
    "    mins = t//60\n",
    "    secs = t%60\n",
    "    tstr=str(mins)+ \":\" +str(secs)\n",
    "    print(tstr)\n",
    "    t-=1\n",
    "    time.sleep(5)\n"
   ]
